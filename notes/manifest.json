{
  "manifest_version": 2,

  "name": "notes",
  "description" : "notes use google driver",
  "version": "1.0.0",

  "browser_action": {
    "default_title": "notes",
    "default_icon": {
      "16": "images/note16.png",
      "24": "images/note24.png",
      "32": "images/note32.png",
      "128": "images/note128.png"
    }
  },
  "icons": { "180": "images/note.png" },
  "key": "",
  "oauth2": {
    "client_id": "955853534139-0uo1oeim5qe5eargckak6kl39anh136t.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive"
    ]
  },
  "permissions": [
    "identity",
    "http://*.google.com/",
    "https://*.google.com/",
    "https://*.gstatic.com/",
    "https://*.googleapis.com/"
  ],
  "background": {
    "scripts":["js/jquery.min.js", "js/gdrive.js", "js/background.js"]
  }
}
